{
  "name": "oa-parser-typescript",
  "version": "0.0.1",
  "description": "OpenAPI parser for Typescript",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint '{src,test}/**/*.ts' --fix",
    "start": "ts-node-dev src/main.ts",
    "start:prod": "node dist/main"
  },
  "author": "Sebastian Copei",
  "license": "MIT",
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
